{% extends 'layout.html.twig' %}

{% block body %}

    <table class="table">
        <thead>
        <tr>
            <th>id</th>
            <th>Name</th>
            <th>Position</th>
            <th>Email</th>
        </tr>
        </thead>
        <tbody>
        {% for team in teams %}
        <tr>

            <th scope="row">{{ team.id }}</th>
            <td>{{ team.name }}</td>
            <td>{{ team.position }}</td>
            <td>{{ team.email }}</td>

        </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
